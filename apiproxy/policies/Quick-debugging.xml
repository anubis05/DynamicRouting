<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Quick-debugging">
    <DisplayName>Quick debugging</DisplayName>
    <Properties/>
    <Add>
        <Headers>
            <Header name="X-CustomHeader">{urirequest.majorVersion}</Header>
        </Headers>
    </Add>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>